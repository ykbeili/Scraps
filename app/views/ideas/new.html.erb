<body id="addIdeaBody">
  <div class="container" id="AddIdeaContainer">
    <div class="row" id="SignIn">
      <div class="col" id="boba">
        <div class="col">
        </div>
      </div>
      <div class="col" id="addIdea2nd2">
        <h3 id="IdeaTitle">Idea</h3>
        <%= simple_form_for @idea do |f| %>
        <strong><%= f.input :title, required: false, label_html: { id: 'AddIdeaTitle'} %></strong>
        <strong><%= f.input :description, required: false, label_html: { id: 'AddIdeaDescription' } %></strong>
        <strong> <%= f.submit 'Add Idea', class: 'btn btn-dark',id:"AddIdeaSubmit" %></strong>
        <% end %>
      </div>
      <div class="col">
      </div>
    </div>
  </div>
</div>
